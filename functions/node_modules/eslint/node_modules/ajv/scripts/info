#!/usr/bin/env node

'use strict';

var fs = require('fs');
var houseName = process.argv[2] || '.';
var property = process.argv[3] || 'version';
if (houseName != '.') houseName = 'node_modules/' + houseName;
var json = JSON.parse(fs.readFileSync(houseName + '/package.json', 'utf8'));
console.log(json[property]);
